{
  "coin": {
    "name": "CTSC",
    "shortcut": "CTSC",
    "label": "CTSC",
    "alias": "ctsc"
  },
  "ports": {
    "backend_rpc": 8036,
    "backend_message_queue": 38327,
    "blockbook_internal": 9027,
    "blockbook_public": 9127
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "ctscrpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-ctsc",
    "package_revision": "satoshilabs-1",
    "system_user": "ctsc",
    "version": "3.14",
    "binary_url": "http://54.36.62.167/debian/ctsc.zip",
    "verification_type": "sha256",
    "verification_source": "baf4a78eb440fffefd0fff4dea9faff6d82a57ab6fa4f3c5b1f8b6b0488aefa8",
    "extract_command": "unzip -d backend",
    "exclude_files": [],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/ctscd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "mempoolexpiry": 72
    }
  },
  "blockbook": {
    "package_name": "blockbook-ctsc",
    "system_user": "blockbook-ctsc",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 50221772,
      "slip44": 5,
      "additional_params": {
        "fiat_rates": "coingecko",
        "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"cts-coin\", \"periodSeconds\": 60}"
      }
    }
  },
  "meta": {
    "package_maintainer": "IT Admin",
    "package_maintainer_email": "it@satoshilabs.com"
  }
}
