{
  "coin": {
    "name": "SAPP",
    "shortcut": "SAPP",
    "label": "SAPP",
    "alias": "sapp"
  },
  "ports": {
    "backend_rpc": 8024,
    "backend_message_queue": 38315,
    "blockbook_internal": 9015,
    "blockbook_public": 9115
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "pivxrpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-sapp",
    "package_revision": "satoshilabs-1",
    "system_user": "sapp",
    "version": "1.3.3.1",
    "binary_url": "https://github.com/sappcoin-com/SAPP/releases/download/v1.3.3.1/SAPP-1.3.3.1-Linux.zip",
    "verification_type": "sha256",
    "verification_source": "8b5faecc7c9a6a9b962865fb4dc6d299b91937c89d461ce00296de7305a49b6e",
    "extract_command": "unzip -d backend",
    "exclude_files": [
      "sap-tx",
      "sap-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/sapd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": false,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "whitelist": "127.0.0.1"
    }
  },
  "blockbook": {
    "package_name": "blockbook-sapp",
    "system_user": "blockbook-sapp",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 36513075,
      "slip44": 119,
      "additional_params": { }
    }
  },
  "meta": {
    "package_maintainer": "rikardwissing",
    "package_maintainer_email": "rikard@coinid.org"
  }
}